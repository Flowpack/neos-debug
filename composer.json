{
    "name": "flowpack/neos-debug",
    "description": "Neos CMS helper package to visualize debug information for page rendering",
    "license": "MIT",
    "type": "neos-plugin",
    "require": {
        "neos/flow": "^8.3.8 || ^9.0",
        "neos/neos": "^8.3 || ^9.0",
        "php": "^8.1"
    },
    "replace": {
        "t3n/neos-debug": "*"
    },
    "autoload": {
        "psr-4": {
            "Flowpack\\Neos\\Debug\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Flowpack.Neos.Debug"
        }
    }
}
