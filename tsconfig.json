{
  "compilerOptions": {
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "target": "ES2020",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "./Resources/Private/Scripts/typings"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
